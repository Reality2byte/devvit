{
  "private": true,
  "name": "community-hub",
  "version": "0.12.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .turbo coverage dist",
    "clobber": "yarn clean && rm -rf node_modules",
    "lint": "redlint .",
    "lint:fix": "yarn lint --fix",
    "test": "yarn test:unit && yarn test:types && yarn lint",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run --passWithNoTests",
    "test:unit-with-coverage": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@devvit/public-api": "0.12.0",
    "dset": "3.1.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@devvit/tsconfig": "0.12.0",
    "typescript": "5.8.3",
    "vitest": "1.6.1"
  }
}
